{
    "name": "money-tracker",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-regular-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@mui/material": "^5.13.0",
        "embla-carousel-react": "^8.0.0-rc07",
        "embla-carousel-wheel-gestures": "^8.0.0-rc04",
        "idb": "^7.1.1",
        "luxon": "^3.3.0",
        "mobx": "^6.9.0",
        "mobx-react-lite": "^4.0.3",
        "mui": "^0.0.1",
        "react": "^18.2.0",
        "react-calendar": "^4.2.1",
        "react-dom": "^18.2.0",
        "react-resizable-panels": "^0.0.54",
        "react-resize-detector": "^9.1.0",
        "react-router-dom": "^6.11.1",
        "ts-pattern": "^5.0.1",
        "typia": "^4.0.9",
        "uplot": "^1.6.24",
        "uplot-react": "^1.1.4",
        "uuid": "^9.0.0",
        "web-vitals": "^3.3.1",
        "zod": "^3.21.4"
    },
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "gen-type-checkers": "rm -rf ./src/typeCheckers.g; typia generate --input ./src/typeCheckers/ --output ./src/typeCheckers.g/",
        "load-currencies": "tsx ./utils/load-currencies.ts",
        "test": "jest",
        "test-coverage": "jest --coverage"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true
        },
        "extends": [
            "plugin:react/recommended",
            "standard-with-typescript"
        ],
        "overrides": [],
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module",
            "project": "./tsconfig.json"
        },
        "plugins": [
            "react",
            "simple-import-sort"
        ],
        "ignorePatterns": [
            "**/*.g/**",
            "dist/**",
            "node_modules/**"
        ],
        "rules": {
            "indent": [
                "error",
                4
            ],
            "@typescript-eslint/indent": [
                "error",
                4
            ],
            "jsx-quotes": [
                "error",
                "prefer-single"
            ],
            "react/jsx-no-literals": [
                "error",
                {
                    "noAttributeStrings": true
                }
            ],
            "simple-import-sort/imports": "error",
            "@typescript-eslint/no-unused-vars": "warn",
            "@typescript-eslint/consistent-type-imports": "warn",
            "@typescript-eslint/no-non-null-assertion": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.1",
        "@types/luxon": "^3.3.0",
        "@types/node": "^20.1.0",
        "@types/react": "^18.2.6",
        "@types/react-dom": "^18.2.4",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@vitejs/plugin-react": "^4.0.0",
        "eslint": "^8.40.0",
        "eslint-config-standard-with-typescript": "^37.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "jest": "^29.6.1",
        "regenerator-runtime": "^0.13.11",
        "sass": "^1.62.1",
        "ts-jest": "^29.1.1",
        "tsx": "^3.12.7",
        "typescript": "^5.1.3",
        "vite": "^4.3.9"
    }
}
